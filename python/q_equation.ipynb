   "source": [
    "import math\n",
    "\n",
    "a = 3\n",
    "b = 6\n",
    "c = 4\n",
    "\n",
    "D = b*b - 4*a*c\n",
    "print(f'D =', D)\n",
    "\n",
    "if D > 0:\n",
    "    x1 = (-b - math.sqrt(D))/(2*a)\n",
    "    x2 = (-b + math.sqrt(D))/(2*a)\n",
    "    print(f'x1 =', x1)\n",
    "    print(f'x2 =', x2)\n",
    "elif D == 0:\n",
    "    x = -b/(2*a)\n",
    "    print(f'x =', x)\n",
    "else:\n",
    "    print(f'Нет решений уравнения')"
   ]
